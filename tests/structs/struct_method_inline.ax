//@test(expect = "value=42")
namespace main

extern c {
    func printf(*byte, ...) int32
}

struct Counter {
    value: int32
    
    func get(self c: Counter) int32 {
        return c.value
    }
}

func main() int32 {

    let counter = Counter{value: 42}
    let val = counter.get()
    printf("value=%d\n", val)

    return 0
}
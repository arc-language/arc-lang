//@test(expect = "x=5 y=10")
namespace main

extern c {
    func printf(*byte, ...) int32
}

struct Point {
    x: int32
    y: int32
}

func move_point(p: *Point, dx: int32, dy: int32) {
    p.x += dx
    p.y += dy
}

func main() int32 {

    let pt = Point{x: 0, y: 0}
    move_point(&pt, 5, 10)
    printf("x=%d y=%d\n", pt.x, pt.y)

    return 0
}